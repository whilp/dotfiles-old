sticky yes

autogroup 1     xterm
autogroup 2     Firefox
autogroup 3     Chrome

gap 0 459 0 0

# Groups.
bind M-u        group1
bind M-i        group2
bind M-o        group3
bind M-p        group4
bind MC-u       movetogroup1
bind MC-i       movetogroup2
bind MC-o       movetogroup3
bind MC-p       movetogroup4

# Other bindings.
bind M-Return   terminal
bind MC-Return  "dmenu_run"
bind M-h        delete
bind M-j        cycle
bind M-k        rcycle
bind M-l        lock
bind M-m        maximize
bind MC-m       hmaximize
bind MS-m       vmaximize
bind MCS-q      "sudo shutdown -hp now"
bind M-r        reload

mousebind M-3   window_resize

# unmap all of the defaults
bind M-Down     unmap
bind M-Up       unmap
bind M-slash    unmap
bind C-slash    unmap
bind CM-n       unmap
bind M-Tab      unmap
bind MS-Tab     unmap
bind CM-x       unmap
bind CM-0       unmap
bind CM-1       unmap
bind CM-2       unmap
bind CM-3       unmap
bind CM-4       unmap
bind CM-5       unmap
bind CM-6       unmap
bind CM-7       unmap
bind CM-8       unmap
bind CM-9       unmap
bind CM-g       unmap
bind M-Right    unmap
bind M-Left     unmap
bind CM-f       unmap
bind M-slash    unmap
bind M-equal    unmap
bind M-minus    unmap
bind M-period   unmap
bind M-comma    unmap
bind CM-w       unmap
bind CMS-r      unmap
bind CMS-q      unmap
bind MS-h       unmap
bind MS-j       unmap
bind MS-k       unmap
bind MS-l       unmap
bind CM-h       unmap
bind CM-j       unmap
bind CM-k       unmap
bind CM-l       unmap
bind CMS-h      unmap
bind CMS-j      unmap
bind CMS-k      unmap
bind CMS-l      unmap
