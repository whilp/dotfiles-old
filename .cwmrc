sticky yes

autogroup 1     xterm
autogroup 2     Firefox
autogroup 3     Chrome

gap 0 424 0 0

# Binding reset; see below for actual bindings.
bind CM-Return unmap
bind CM-Delete unmap
bind M-Return unmap
bind M-Down unmap
bind M-Up unmap
bind M-/ unmap
bind C-/ unmap
bind CM-n unmap
bind M-Tab unmap
bind MS-Tab unmap
bind CM-x unmap
bind CM-[n] unmap
bind CM-0 unmap
bind CM-g unmap
bind M-Right unmap
bind M-Left unmap
bind CM-f unmap
bind CM-= unmap
bind CMS-= unmap
bind M-? unmap
bind M-. unmap
bind CM-w unmap
bind CMS-r unmap
bind CMS-q unmap
bind M-M1 unmap
bind CM-M1 unmap
bind M-M2 unmap
bind M-M3 unmap
bind CMS-M3 unmap

# Groups.
bind 4C-u       group1
bind 4C-i       group2
bind 4C-o       group3
bind 4C-p       group4
bind 4CS-u      movetogroup1
bind 4CS-i      movetogroup2
bind 4CS-o      movetogroup3
bind 4CS-p      movetogroup4

# Other bindings.
bind 4-Return   "cwmenu"
bind 4-x        delete
bind 4-j        cycle
bind 4-k        rcycle
bind 4-q        "mpdlock"
bind 4-m        maximize
bind 4-s        hide
bind 4C-m       hmaximize
bind 4S-m       vmaximize
bind 4-r        reload
bind 4-1        "mpdlock"
bind 4-5        "mpc -q pause"
bind 4-6        "mpc -q previous"
bind 4-7        "mpc -q next"
bind 4-8        "mpc -q toggleplay"
bind 4-10       "mixerctl -q outputs.master.mute=toggle"
bind 4-11       "mixerctl -q outputs.master=-12"
bind 4-12       "mixerctl -q outputs.master=+12"

mousebind 4-2   window_lower
mousebind 4-3   window_resize

# unmap all of the defaults
bind MC-Return  unmap
bind M-Down     unmap
bind M-Up       unmap
bind M-slash    unmap
bind C-slash    unmap
bind M-n        unmap
bind M-p        unmap
bind MC-n       unmap
bind M-Tab      unmap
bind MS-Tab     unmap
bind MC-x       unmap
bind MC-0       unmap
bind MC-1       unmap
bind MC-2       unmap
bind MC-3       unmap
bind MC-4       unmap
bind MC-5       unmap
bind MC-6       unmap
bind MC-7       unmap
bind MC-8       unmap
bind MC-9       unmap
bind MC-g       unmap
bind M-Right    unmap
bind M-Left     unmap
bind MC-f       unmap
bind M-slash    unmap
bind M-equal    unmap
bind M-minus    unmap
bind M-period   unmap
bind M-comma    unmap
bind MC-w       unmap
bind MCS-r      unmap
bind MCS-q      unmap
bind M-h        unmap
bind M-j        unmap
bind M-k        unmap
bind M-l        unmap
bind MS-h       unmap
bind MS-j       unmap
bind MS-k       unmap
bind MS-l       unmap
bind MCS-h      unmap
bind MCS-j      unmap
bind MCS-k      unmap
bind MCS-l      unmap
