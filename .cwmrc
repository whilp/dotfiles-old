sticky yes

autogroup 1     xterm
autogroup 2     Firefox
autogroup 3     Chrome

gap 0 424 0 0

# Binding reset; see below for actual bindings.
bind C-Down unmap
bind C-Left unmap
bind C-Right unmap
bind C-Up unmap
bind C-slash unmap
bind CM-0 unmap
bind CM-1 unmap
bind CM-2 unmap
bind CM-3 unmap
bind CM-4 unmap
bind CM-5 unmap
bind CM-6 unmap
bind CM-7 unmap
bind CM-8 unmap
bind CM-9 unmap
bind CM-Delete unmap
bind CM-M1 unmap
bind CM-Return unmap
bind CM-equal unmap
bind CM-f unmap
bind CM-g unmap
bind CM-h unmap
bind CM-j unmap
bind CM-k unmap
bind CM-l unmap
bind CM-n unmap
bind CM-w unmap
bind CM-x unmap
bind CMS-M3 unmap
bind CM-equal unmap
bind CM-Q unmap
bind CM-R unmap
bind CS-Down unmap
bind CS-Left unmap
bind CS-Right unmap
bind CS-Up unmap
bind M-Down unmap
bind M-Left unmap
bind M-M1 unmap
bind M-M2 unmap
bind M-M3 unmap
bind M-Return unmap
bind M-Right unmap
bind M-Tab unmap
bind M-Up unmap
bind M-h unmap
bind M-j unmap
bind M-k unmap
bind M-l unmap
bind M-period unmap
bind M-question unmap
bind M-slash unmap
bind MS-Tab unmap
bind M-H unmap
bind M-J unmap
bind M-K unmap
bind M-L unmap

# Groups.
bind 4-u       group1
bind 4-i       group2
bind 4-o       group3
bind 4-p       group4
bind 4C-u      movetogroup1
bind 4C-i      movetogroup2
bind 4C-o      movetogroup3
bind 4C-p      movetogroup4

# Other bindings.
bind 4-Return   "cwmenu"
bind 4-period   delete
bind 4-j        cycle
bind 4-k        rcycle
bind 4-q        "mpdlock"
bind 4-m        maximize
bind 4C-m       hmaximize
bind 4-M        vmaximize
bind 4-r        reload
bind 4-1        "mpdlock"
bind 4-5        "mpc -q pause"
bind 4-6        "mpc -q previous"
bind 4-7        "mpc -q next"
bind 4-8        "mpc -q toggleplay"
bind 4-10       "mixerctl -q outputs.master.mute=toggle"
bind 4-11       "mixerctl -q outputs.master=-12"
bind 4-12       "mixerctl -q outputs.master=+12"

mousebind 4-2   window_lower
mousebind 4-3   window_resize
