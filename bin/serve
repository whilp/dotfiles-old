#!/usr/bin/env python

from SimpleHTTPServer import (
    SimpleHTTPRequestHandler,
    BaseHTTPServer)

SimpleHTTPRequestHandler.extensions_map.update({
    ".svg": "image/svg+xml",
})

try:
    BaseHTTPServer.test(
        SimpleHTTPRequestHandler,
        BaseHTTPServer.HTTPServer)
except (KeyboardInterrupt):
    pass
