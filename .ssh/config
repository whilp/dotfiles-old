Host *
    Compression     yes
    #ConnectTimeout  5
    ControlPath     ~/.ssh/ctl-%r-%h-%p
    ControlMaster   auto
    #ControlPersist  1h
    ForwardAgent    yes
    ForwardX11      no
    Protocol        2
    EscapeChar      !
    HashKnownHosts  yes
    ServerAliveInterval 30
