Host admin
    HostName    cmsdcache02.hep.wisc.edu
    Port        22223
    Protocol    1
    Cipher      blowfish
    User        admin
    ForwardAgent    no
    IdentityFile    ~/.ssh/identity

Host *
    ControlPath ~/.ssh/ctl-%r-%h-%p
    ControlMaster auto
    ForwardAgent yes
    ForwardX11  no
    Protocol    2
    EscapeChar  !
    HashKnownHosts yes
